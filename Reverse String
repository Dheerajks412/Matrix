// "static void main" must be defined in a public class.
public class Main {
    public static void main(String[] args) {
        String name = "DHEERAJ";
        String reverse = " ";
        for(int i=name.length()-1; i>=0; i--){
            reverse = reverse + name.charAt(i);
        }
        System.out.print(reverse);
    }
}
