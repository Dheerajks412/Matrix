class Node{
    int num;
    Node next;
    Node(int n){
        this.num = n;
        this.next = null;
    }
}
public class TwoPointer{
    public void findNode(Node n){
        if(n==null){
            return;
        }
        
        Node slow = n;
        Node fast = n;
        while(fast != null && fast.next != null){
            fast = fast.next.next;
            slow = slow.next;
        }
            System.out.println("The middle node of the linked list " + slow.num);
    }
    
    public static void main(String[] args){
        Node h = new Node(15);
        h.next = new Node(11);
        h.next.next = new Node(12);
        h.next.next.next = new Node(13);
        h.next.next.next.next = new Node(16);
        h.next.next.next.next.next = new Node(18);
        TwoPointer obj = new TwoPointer();
        obj.findNode(h);
    }
}
